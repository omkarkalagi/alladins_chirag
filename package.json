{
  "name": "alladins-chirag",
  "version": "1.0.0",
  "description": "Trading dashboard with client-server architecture",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "server": "node server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.0",
    "nodemailer": "^6.9.0",
    "react-icons": "^5.5.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^6.0.1",
    "sourcemap-codec": "^1.4.8",
    "stripe": "^12.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  },
  "keywords": [
    "trading",
    "dashboard",
    "react",
    "express",
    "mongodb"
  ],
  "author": "omkarkalagi",
  "license": "MIT"
}
