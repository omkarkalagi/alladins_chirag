{
  "name": "alladins-chirag",
  "version": "1.0.0",
  "description": "Trading dashboard with client-server architecture",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "server": "node server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "express": "^4.18.0",
    "mongoose": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.0",
    "stripe": "^12.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  },
  "keywords": ["trading", "dashboard", "react", "express", "mongodb"],
  "author": "omkarkalagi",
  "license": "MIT"
}